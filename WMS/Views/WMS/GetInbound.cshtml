
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="modal-shiftfix">
    <div class="container-fluid main-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="widget-container fluid-height clearfix">
                    <div class="heading">
                        <i class="icon-table"></i>资产调度记录
                    </div>
                    <div class="widget-content padded clearfix">
                        <table class="table table-bordered table-striped" id="dataTable1">
                            <thead>
                            <th>
                                入库记录Id
                            </th>
                            <th>
                                入库产品
                            </th>
                            <th class="hidden-xs">
                                入库数量
                            </th>
                            <th class="hidden-xs">
                                入库仓库
                            </th>
                            <th class="hidden-xs">
                                入库时间
                            </th>
                            <th>
                                出库清单
                            </th>
                            <tbody id="tby"></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    $.ajax({
        url: "http://localhost:60847/api/KHZ/ShowInbound",
        success: function (data) {
            $(data).each(function () {
                var tr = "<tr>" +
                    "<td>" + this.Id + "</td>" +
                    "<td>" + this.ProductTypeName + "</td>" +
                    "<td>" + this.Quantity + "</td>" +
                    "<td>" + this.WarehouseName + "</td>" +
                    "<td>" + this.StartTime + "</td>" +
                    "<td>" + this.Contract + "</td>" +
                    "</tr >";
                $("#tby").append(tr);
            })
        }
    })
</script>